}

RecordChangeLog: {
  shape: sql_table

  id: string {constraint: [primary_key; unique]}
  blipId: string {constraint: foreign_key}
  previousRecord: string
  name: string
  quadrant: string
  ring: string
  description: string
  created_at: timestamp
}

